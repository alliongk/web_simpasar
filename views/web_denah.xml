<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="web_denah" model="website.menu">
            <field name="name">Denah Pasar</field>
            <field name="url">/denah</field>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence" type="int">20</field>
        </record>

        <template id="denah" name="denah">
            <t t-call="website.layout">
                <div id="wrap" class="oe_structure oe_empty">
                         
                    <!-- view maps -->
                    <section class="s_picture pt48 pb24 o_cc o_cc5 o_colored_level" data-snippet="s_picture" data-name="Picture" style="background-image: none;">
                        <div class="container">
                            <h2 style="text-align: center;"><font style="font-size: 62px;" class="o_default_snippet_text">Denah Pasar</font></h2>
                            <p style="text-align: center;"><br/></p>
                            <div class="row s_nb_column_fixed">
                                <div class="col-lg-10 offset-lg-1 pb24 o_colored_level" style="text-align: center;">
                                    <section class="s_map pb56 pt56" data-map-type="m" data-map-zoom="12" data-snippet="s_map" data-map-address="Indonesia" data-name="Map">
                                        <div class="map_container o_not_editable">
                                            <div class="css_non_editable_mode_hidden">
                                                <div class="missing_option_warning alert alert-info rounded-0 fade show d-none d-print-none o_default_snippet_text">
                                                    An address must be specified for a map to be embedded
                                                </div>
                                            </div>
                                            
                                            <iframe class="s_map_embedded o_not_editable" width="100%" height="100%" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://maps.google.com/maps?q=Indonesia&amp;t=m&amp;z=12&amp;ie=UTF8&amp;iwloc=&amp;output=embed"/><div class="s_map_color_filter"/>
                                        </div>
                                    </section>
                                </div>
                            </div>
                        </div>
                    </section> 

                    <section class="s_banner parallax s_parallax_is_fixed pt96 pb96 o_cc5 o_colored_level" data-scroll-background-ratio="1" data-oe-shape-data="{&quot;shape&quot;:&quot;web_editor/Wavy/02_001&quot;,&quot;flip&quot;:[&quot;y&quot;]}" data-snippet="s_banner" data-name="Banner" style="background-image: none;">
                        <span class="s_parallax_bg oe_img_bg" style="background-image: url('/web_simpasar/static/src/img/pasarlamongan_bgd.jpg'); background-position: 50% 0;"/>
                        <div class="o_we_bg_filter" style="background-color: rgba(17, 13, 22, 0.85) !important;"/>
                        <div class="o_we_shape o_web_editor_Wavy_02_001" style="background-image: url(&quot;/web_editor/shape/web_editor%2FWavy%2F02_001.svg?c3=%23142a2e&amp;flip=y&quot;); background-position: 50% 100%;"/>
                        <div class="container">
                            <div class="row s_nb_column_fixed">
                                <div class="col-lg-6 jumbotron rounded pt32 pb32 o_colored_level" data-name="Box">
                                    <p><br/></p>
                                    <p><br/></p>
                                    <p><br/></p>
                                <p><t t-esc="denah.name_denah" /></p>
                                </div>
                            </div>
                        </div>
                    </section>    
                </div>
            </t>
        </template>
    </data>
</odoo>
