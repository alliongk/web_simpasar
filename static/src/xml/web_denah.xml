<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="web_denah" model="website.menu">
          <field name="name">Denah Pasar</field>
          <field name="url">/denah</field>
          <field name="parent_id" ref="website.main_menu"/>
          <field name="sequence" type="int">20</field>
      
        </record>

        <template id="denah" name="denah">
            <t t-call="website.layout">
                <div id="wrap" class="oe_structure oe_empty">
                    <div class="container" style="margin-top:5%; margin-bottom:5%">
                        <t t-foreach="data" t-as="data_row">
                            <div class="row" style="margin-bottom:15px;">
                                <t t-foreach="data_row" t-as="data_denah">
                                    <div class="col-md-6">
                                        <div class="card bg-white">
                                            <t t-if="data_denah.denah">
                                                <a t-attf-href="data:image/png;base64,{{data_denah.denah}}" target="_blank">
                                                    <img class="card-img-top" t-attf-src="data:image/png;base64,{{data_denah.denah}}"/>
                                                </a>
                                            </t>
                                            <t t-if="not data_denah.denah">
                                                <img class="card-img-top" src='/pasar/static/img/Untitled.png' />
                                            </t>
                                            <div class="card-body">
                                                <h4 class="card-title">Denah <t t-esc="data_denah.name"/></h4>
                                            </div>
                                        </div>
                                    </div>
                                </t>
                            </div>
                        </t>
                    </div>
                </div>
            </t>
        </template>
    </data>
</odoo>